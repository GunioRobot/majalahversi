/* put your custom CSS in here */

body {
  background: #666 url(../images/bg.png) repeat-y scroll 50% 0;
  
}
.container {
  background-color: #EEE;
  width: 980px;
  border: 0px solid #666;
}

#feedback-tab {
  background: #222 url(../images/feedback-tab.png);
  color:#FFFFFF;
  cursor:pointer;
  height:102px;
  left:0;
  margin-left:-7px;
  overflow:hidden;
  position:fixed;
  text-indent:-100000px;
  top:150px;
  width:42px;
  z-index:100000;
  border: 0px solid #CCC;
}

div.container {
  margin: 0 auto;
}

h1,h2,h3 { font-family: 'Georgia', 'Times New Roman'; }

a:link  { 
  color: #C00; 
  text-decoration: none; 
}

a:visited { 
  color: #999;
  text-decoration: none; 
}

a:focus,
a:hover { 
  color: #C00;
  text-decoration: underline; 
}

a:active  { 
  color: #C00;
  text-decoration: underline; 
}

/*
::selection         { background:#c3effd; color:#000; Safari and Opera  }
::-moz-selection  { background:#c3effd; color:#000; Firefox}
*/

ul.primary {
  border-bottom:0px solid #BBBBBB;
  list-style-image:none;
  list-style-position:outside;
  list-style-type:none;
  margin:0 0 0 0;
  padding:0;
}

div.tabs {
  padding: 5px 0 20px 0;
  margin: 0;
}

ul.primary li a, ul.primary li.active a {
  border: 0;
}

ul.primary li.active a {
  background-color: #666;
  color: #CCC;
}

ul.primary li a:hover {
  background-color: #FFF;
}

ul.primary li a {
  padding: 5px 10px;
  margin: 0 0px 0 0;
}

#right div.block {
  position: relative;
  padding: 40px 20px 20px 20px;
  background-color: #FFF;
  border: solid 0px #AAA;
  margin-bottom: 20px;
}

#right div.block h3 {
  padding: 20px 0 10px 0;
  border-bottom: solid 0px #CCC;
  margin:0;
  float: left;
}

#right div.block .views-field-teaser p {
  clear: left;
  margin-bottom: 0;
}


#right ul.menu, #right div.item-list ul {
  padding: 0;
  margin: 0;
}

#right ul.menu a, #right div.item-list ul a {
  text-decoration: none;
}

#right ul.menu a:hover, #right div.item-list ul a:hover {
  background-color: #F6F6F6;
}

#right ul.menu li, #right div.item-list ul li {
  list-style-image: none;
  list-style-type: none;
}

#right ul.menu li a,
#right ul.menu li.leaf a,
#right div.item-list ul li a,
#right div.item-list ul li.leaf a
{
  padding: 5px 0 5px 10px;
  border-bottom: 1px solid #EEE;
  display: block;
}

#right ul.menu li li a,
#right ul.menu li li.leaf a {
  padding: 5px 0 5px 30px;
  border-bottom: 1px solid #EEE;
  display: block;
}

#right ul.menu li li li a,
#right ul.menu li li li.leaf a {
  padding: 5px 0 5px 50px;
  border-bottom: 1px solid #EEE;
  display: block;
}

#right ul.menu li li li li a,
#right ul.menu li li li li.leaf a {
  padding: 5px 0 5px 70px;
  border-bottom: 1px solid #EEE;
  display: block;
}

#right ul.menu li li li li li a,
#right ul.menu li li li li li.leaf a {
  padding: 5px 0 5px 90px;
  border-bottom: 1px solid #EEE;
  display: block;
}

#right ul.menu a.active {
  /*background: #EEE url(../images/02.png) no-repeat scroll 97% 50%;*/
  color: #333;
  text-decoration: none;
  font-weight: bold;
}

#right div.item-list ul li div.views-field-timestamp {
  text-align: right;
  font-style: italic;
  font-size: .8em;
  margin-bottom: 10px;
}

#right div.item-list ul li {
  position: relative;
}

