div.views-row-last div.node { margin-bottom: 0; }

#center div.node, #content-middle-one div.node {
	background-color: #FFF;
	border-bottom: 0;
	margin-bottom: 0;
	padding: 20px; }


#content-bottom {
	background-color: #1D1D1D;
	margin-bottom: 0;
	padding: 20px 0 0 20px; }

div.view-content div.node, div#content-content div.node {
	position: relative;
	/*background: #FFF url(../images/box-bgr.gif) repeat-x center top;*/ }

div.node .categories ul{
    text-align: left;
    text-transform: uppercase;
    margin: 0 0 10px 0; }

div.node .categories ul li a{
    letter-spacing: .5em;    
    color: #C00;
}
.content .tags {
    font-style: italic;
}

p.node-submit-info {
    color: #C00;
}

div.terms {
	background-color: #1D1D1D;
	display: block;
	font-size: 1.2em;
/*	opacity: 0.75;*/
	padding: 5px;
	position: absolute;
	right: 0;
	text-transform: uppercase;
	top: 0; }

div.meta {
	border-bottom: 0px solid #CCC;
	/*padding-bottom: 10px;*/ }

div.terms ul.links li { padding: 0; }

	div.terms ul.links li a {
		color: #FFFFFF;
		font-weight: bold; }

div.comment {
	background-color: #F7F7F7;
	margin: 0 0 20px 0;
	padding: 20px; }

div.comment-content {
	float: right;
	margin: 0;
	padding: 0;
	position: relative;
	width: 455px; }

div.preview div.comment-content { width: 415px; }

div.comment.odd { background-color: #FFF; }

div.comment.author { background-color: #FFF; }

	div.comment.author img {
		background-color: #333;
		border: 1px solid #333;
		padding: 9px; }

div.comment-picture {
	border: 0;
	float: left;
	margin-right: 20px;
	padding: 0; }

	div.comment-picture img {
		background-color: #F6F6F6;
		border: 1px solid #CCC;
		padding: 9px; }

div.comment span.new {
	background-color: #CC0000;
	color: #FFF;
	display: block;
	padding: 4px;
	position: absolute;
	right: -25px;
	top: -25px; }

#comment-form, #filter-admin-format-form {
	background-color: #FFF;
	margin-bottom: 20px;
	padding: 20px; }

div.box {
	border-bottom: 1px solid #CCC;
	margin-bottom: 0;
	padding: 20px; }

	div.box h3 { margin-bottom: 0; }

h3#comment-number {
	background-color: #F6F6F6;
	border-bottom: 1px solid #CCC;
	margin-bottom: 0px;
	padding: 20px; }

