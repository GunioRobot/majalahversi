ul.links li { padding: 0 10px 0 0; }

.book-navigation .page-links { background-color: #EEE; }

ul.jquerymenu li span.parent {
	height: 10px;
	margin-left:-5px;
	margin-top:8px;
	width: 10px; }

#right ul.menu li ul { margin:0; }

ul.jquerymenu li.parent span.closed { background-image:url(../images/29.png); }

ul.jquerymenu li.parent span.open { background-image:url(../images/30.png); }

ul.custom-pager {
  margin-top: 20px;
}

ul.pager li.pager-current {
  background-color: #111;
}

ul.pager a:link, ul.pager a:visited {
  color: #111;
  background-color: #FFF;
}

ul.pager a, ul.pager li.pager-current {
  border: 0;
}

ul.pager a:hover, ul.pager a:focus, ul.pager a:active {
  border: 0;
  background-color: #111;
  color: #FFF;
}

#footer {
	background-color: #1D1D1D;
	width: 980px; 
    border-top: 1px solid #333; }

	#footer ul.menu {
		margin: 0;
		padding: 10px; }

		#footer ul.menu li {
			float: left;
			list-style: none; }

		#footer ul.menu a {
		    color: #CCC;
			border-right:1px solid #CCC;
			display:block;
			padding:0 10px; }

			#footer ul.menu a:link { text-decoration:none; }

			#footer ul.menu a:visited { text-decoration:none; }

			#footer ul.menu a:hover { text-decoration:underline; }

		#footer ul.menu li a {
			font-size:1em;
			line-height:1.875em; }

		#footer ul.menu li.first a { padding-left:0; }

		#footer ul.menu li.last a { border-right:0; }


